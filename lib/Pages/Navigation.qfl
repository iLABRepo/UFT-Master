Dim browserObjNav
Set browserObjNav = Browser("title:=.*Wits Apps.*").Page("title:=.*Wits Apps.*")
Function ClickProfileDropdown()
    browserObjNav.WebElement("xpath:=//span[@class='text-dark']").Click
End Function
Function ClickLogout()
    browserObjNav.Link("xpath:=//li[@class='sidebar-item']//a[@class='sidebar-link']").Click
End Function
Function ClickActionsRequired()
    browserObjNav.Link("xpath:=//LI[@id='actionsRequiredLi']/A").Click
End Function
